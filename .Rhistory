# Load Data
X <- read.csv(paste0(data,"X_3TC.csv"), header=FALSE)
Y <- read.csv(paste0(data,"Y_3TC.csv"), header=FALSE)
names(Y) <- c("Y")
df <- as.data.frame(cbind(X,Y))
# Obtain OLS coefficients & estimated error variance
lm.3TC <- lm(Y ~ ., data = df)
sigma <- summary(lm.3TC)$sigma
# Generate Y based on OLS coefficients
Y.gen <- predict(lm.3TC) + rnorm(633, mean = 0, sd = sigma)
View(Y)
View(Y)
df.x <- as.data.frame(X)
?lm.ridge
library(MASS)
?lm.ridge
df.gen <- as.data.frame(cbind(X,Y.gen))
ridge.3TC <- lm.ridge(Y.gen ~ ., data=df.gen, lambda=seq(0,10,0.05))
plot(ridge.3TC, lwd=3)
plot(ridge.3TC$lambda, ridge.3TC$GCV, xlab='Lambda', ylab='GCV', type='l', lwd=3, col='orange')
